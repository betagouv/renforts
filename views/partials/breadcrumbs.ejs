<nav role="navigation" class="rf-breadcrumb" aria-label="vous êtes ici :">
  <button class="rf-breadcrumb__button" aria-expanded="false" aria-controls="breadcrumb-189">Voir le fil
    d’Ariane</button>
  <div class="rf-collapse" id="breadcrumb-189">
    <ol class="rf-breadcrumb__list">
      <!-- Display the parent pages, except current page -->
      <% for (var i=0; i < items.length - 1; i++) { %>
        <li>
          <% if (items[i].url) { %>
            <a class="rf-breadcrumb__link" href="<%= items[i].url %>">
              <%= items[i].label %>
            </a>
            <% } else { %>
              <%= items[i].label %>
                <% } %>
        </li>
        <% } %>
          <!-- Display the current page -->
          <li class="rf-breadcrumb__item rf-breadcrumb__item--current" aria-current="page">
            <%= items[items.length - 1].label %>
          </li>
    </ol>
  </div>
</nav>